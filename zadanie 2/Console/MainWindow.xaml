<Window x:Class="Console.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Console"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Height="35" Margin="40,60,0,0" TextWrapping="Wrap" Text="Wybór opcji:" VerticalAlignment="Top" Width="105"/>
        <ComboBox x:Name="combobox" HorizontalAlignment="Left" Height="40" Margin="135,47,0,0" VerticalAlignment="Top" Width="415" SelectionChanged="combobox_SelectionChanged" />
        <TextBlock HorizontalAlignment="Left" Height="35" Margin="40,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="105"><Run Text="L"/><Run Text="iczba początkowa:"/></TextBlock>
        <TextBox x:Name="liczba_poczatkowa" HorizontalAlignment="Left" Height="30" Margin="159,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="175" TextChanged="TextBox_TextChanged"/>
        <TextBlock x:Name="wynik" HorizontalAlignment="Left" Height="60" Margin="159,200,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="175"/>




    </Grid>
</Window>
