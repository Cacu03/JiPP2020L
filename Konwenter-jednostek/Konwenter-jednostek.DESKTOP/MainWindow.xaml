<Window x:Class="Konwenter_jednostek.DESKTOP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Konwenter_jednostek.DESKTOP"
        mc:Ignorable="d"
        Title="Konwenter Jednostek" Height="450" Width="800">
    <Grid>
        <Label x:Name="temperaturalabel" Content="Konwenter Temperatury" HorizontalAlignment="Left" Margin="53,27,0,0" VerticalAlignment="Top"/>
        <Label x:Name="masalabel" Content="Konwenter Masy" HorizontalAlignment="Left" Margin="53,58,0,0" VerticalAlignment="Top"/>
        <Label x:Name="dlugosclabel" Content="Konwenter Dlugosci" HorizontalAlignment="Left" Margin="53,89,0,0" VerticalAlignment="Top"/>
        <Label x:Name="cisnienielabel" Content="Konwenter Cisnienia" HorizontalAlignment="Left" Margin="53,120,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="masaComboBox" HorizontalAlignment="Left" Margin="319,63,0,0" VerticalAlignment="Top" Width="155" SelectionChanged="masaComboBox_SelectionChanged"/>
        <ComboBox x:Name="temperaturaComboBox" HorizontalAlignment="Left" Margin="319,31,0,0" VerticalAlignment="Top" Width="155" SelectionChanged="temperaturaComboBox_SelectionChanged"/>
        <ComboBox x:Name="dlugoscComboBox" HorizontalAlignment="Left" Margin="319,93,0,0" VerticalAlignment="Top" Width="155" SelectionChanged="dlugoscComboBox_SelectionChanged"/>
        <ComboBox x:Name="cisnienieComboBox" HorizontalAlignment="Left" Margin="319,124,0,0" VerticalAlignment="Top" Width="155" SelectionChanged="cisnienieComboBox_SelectionChanged"/>
        <TextBox x:Name="temperaturaTextBox" HorizontalAlignment="Left" Height="23" Margin="194,31,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" TextChanged="temperaturaTextBox_TextChanged"/>
        <TextBox x:Name="masaTextBox" HorizontalAlignment="Left" Height="23" Margin="194,62,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55"/>
        <TextBox x:Name="dlugoscTextBox" HorizontalAlignment="Left" Height="23" Margin="194,93,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" TextChanged="dlugoscTextBox_TextChanged"/>
        <TextBox x:Name="cisnienieTextBox" HorizontalAlignment="Left" Height="23" Margin="194,125,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55"/>
        <TextBlock x:Name="cisnienieTextBlock" HorizontalAlignment="Left" Margin="479,130,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <TextBlock x:Name="dlugoscTextBlock" HorizontalAlignment="Left" Margin="479,94,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <TextBlock x:Name="masaTextBlock" HorizontalAlignment="Left" Margin="479,63,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <TextBlock x:Name="temperaturaTextBlock" HorizontalAlignment="Left" Margin="479,32,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.25,0.731"/>
        <TextBlock x:Name="jednostkatemperaturyTextBlock" HorizontalAlignment="Left" Margin="254,35,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.25,0.731"/>
        <TextBlock x:Name="jednostkamasy_TextBlock" HorizontalAlignment="Left" Margin="254,66,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.25,0.731"/>
        <TextBlock x:Name="jednostkadlugosci_TextBlock" HorizontalAlignment="Left" Margin="254,97,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.25,0.731"/>
        <TextBlock x:Name="jednostkacisnienia_TextBlock" HorizontalAlignment="Left" Margin="254,130,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.25,0.731"/>
    </Grid>
</Window>
