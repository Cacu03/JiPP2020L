        <TextBox x:Name="Konwersja" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Margin="173,110,0,0"/>
        <TextBox x:Name="Konwerter" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="173,38,0,0"/>
