<UserControl x:Class="contest.random"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:contest"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200">
    <Grid Background="Black">
        <Ellipse Height="200" Stroke="Black" Width="200" Fill="#FF98FF8E"/>
        <Ellipse Height="150" Stroke="Black" Width="150" Fill="SkyBlue"/>
        <Ellipse Height="100" Stroke="Black" Width="100" Fill="#FF98FF8E"/>
        <TextBlock x:Name="generator" TextWrapping="Wrap" Height="50" Width="50" FontSize="30" />
    </Grid>
</UserControl>
