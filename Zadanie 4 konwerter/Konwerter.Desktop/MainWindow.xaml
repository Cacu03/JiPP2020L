        <DatePicker x:Name="DATA_OD" HorizontalAlignment="Left" Margin="173,274,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="DATA_DO" HorizontalAlignment="Left" Margin="309,274,0,0" VerticalAlignment="Top"/>
        <Label Content="od" HorizontalAlignment="Left" Margin="144,273,0,0" VerticalAlignment="Top"/>
