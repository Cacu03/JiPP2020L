        <Path x:Name="Minutnik" Data="M434,145.35351 L434,102.08178" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="44.272" Margin="434,102.082,0,0" Stretch="Fill" Stroke="Black" VerticalAlignment="Top" Width="1" RenderTransformOrigin="0,1">
            <Path.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform x:Name="RotateMinutnik" Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Path.RenderTransform>
        </Path>
        <Path x:Name="Godzinowa" Data="M434,145.35351 L434,102.08178" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="35.272" Margin="434.2,110.15,0,0" Stretch="Fill" Stroke="Black" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0,1">
