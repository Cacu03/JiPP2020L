        <Button x:Name="Top5Button" Content="TOP 5" HorizontalAlignment="Left" Margin="496,274,0,0" VerticalAlignment="Top" Width="79" Height="24" Click="Top5Button_Click"/>
        <Label x:Name="page" Content="1" HorizontalAlignment="Left" Margin="309,556,0,0" VerticalAlignment="Top" Height="32" Width="22" RenderTransformOrigin="1.727,0.531"/>
