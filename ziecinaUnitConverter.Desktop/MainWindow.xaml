        <Storyboard x:Key="clockMoving">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="handHour">
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="clockMovingMinute">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="handMinute">
                <EasingDoubleKeyFrame KeyTime="1:0:0" Value="360"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="clockMovingSecond">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)" Storyboard.TargetName="handSecond">
                <EasingDoubleKeyFrame KeyTime="0:1:0" Value="360"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard Storyboard="{StaticResource clockMoving}"/>
            <BeginStoryboard Storyboard="{StaticResource clockMovingMinute}"/>
            <BeginStoryboard Storyboard="{StaticResource clockMovingSecond}"/>
        </EventTrigger>
    </Window.Triggers>
